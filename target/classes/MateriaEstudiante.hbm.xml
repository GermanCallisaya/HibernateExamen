<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "https://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping package="com.hibernate.model">
    <class name="Materia" table="estudiantemateria" schema="public">
        <property name="id_estudiante" column="id_estudiante" />
        <property name="id_materia" column="id_materia" />
        <property name="calificacion" column="calificacion" />
 

    </class> 
</hibernate-mapping>